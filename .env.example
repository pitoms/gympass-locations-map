# Gympass Authentication
GYMPASS_BEARER_TOKEN=your_bearer_token_here

# Update this token when it expires (check JWT exp field)
# Token format: eyJhbGciOiJIUzI1NiIsInR5cCI... (without "Bearer" prefix)

# Google Maps API Key (required - for geocoding and map display)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Vite environment variable for Google Maps (same key as above)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
